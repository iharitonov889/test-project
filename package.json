    "nodemailer": "^7.0.5",
    "nodemon": "^3.1.10",
    "pg": "^8.16.3",
    "postgre": "^0.1.8",
    "quasar": "^2.16.0",
    "redis": "^5.6.0",
    "sequelize": "^6.37.7",
    "vue": "^3.4.18",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@quasar/app-vite": "^2.1.0",
